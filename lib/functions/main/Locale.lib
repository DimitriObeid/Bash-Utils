# Checking if the language's code matches to a valid ISO 639-1 code.
function BU.Main.Locale.CheckISO_639_1_ValidCode()
{
    [[ ${__BU_MAIN_LOCALE_ISO_639_1_LOCALES_ARRAY[*]} =~ ${1,,} ]] && return 0; return 1;
}

